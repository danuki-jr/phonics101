{
  "name": "symfony/website-skeleton",
  "type": "project",
  "license": "MIT",
  "description": "A skeleton to start a new Symfony website",
  "require": {
    "ext-gd": "*",
    "ext-json": "*",
    "ext-mysqli": "*",
    "scssphp/scssphp": "^1.1",
    "symfony/console": "^5.1",
    "symfony/dotenv": "^5.1",
    "symfony/flex": "^1.6",
    "symfony/framework-bundle": "^5.1",
    "symfony/http-kernel": "5.1.*",
    "symfony/phpunit-bridge": "^3.4.3",
    "twig/twig": "^3.0",
    "ext-pdo": "*",
    "ext-readline": "*"
  },
  "require-dev": {
    "ext-curl": "*",
    "phpunit/phpunit": "^9.4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "files": [
      "src/3rdParty/libChart/classes/libChart.php"
    ],
    "psr-4": {
      "Peekmo\\": "src/3rdParty/Peekmo/",
      "JsonMapper\\": "src/3rdParty/JsonMapper",
      "App\\": "src/",
      "Tests\\":"tests/"
    }
  },
  "autoload-dev": {
    "psr-4": {
    }
  },
  "scripts": {
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.1.*"
    }
  },
  "conflict": {
    "symfony/symfony": "5.1.*"
  }
}

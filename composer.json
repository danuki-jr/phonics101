{
  "name": "symfony/website-skeleton",
  "type": "project",
  "license": "MIT",
  "description": "A skeleton to start a new Symfony website",
  "require": {
    "ext-gd": "*",
    "ext-json": "*",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "ext-readline": "*",
    "myclabs/php-enum": "^1.7",
    "scssphp/scssphp": "^1.1",
    "symfony/config": "5.1.*",
    "symfony/console": "^5.1",
    "symfony/dotenv": "^5.1",
    "symfony/flex": "^1.6",
    "symfony/framework-bundle": "^5.1",
    "symfony/http-kernel": "5.1.*",
    "symfony/phpunit-bridge": "^3.4.3",
    "symfony/yaml": "*",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "ext-curl": "*",
    "codeception/codeception": "^4.1",
    "codeception/module-phpbrowser": "^1.0",
    "codeception/module-webdriver": "^1.2",
    "phpunit/phpunit": "^9.4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "files": [
      "src/3rdParty/libChart/classes/libChart.php"
    ],
    "psr-4": {
      "Peekmo\\": "src/3rdParty/Peekmo/",
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\":"tests/"
    },
    "psr-0": {"App\\Tests\\": "tests/_support"}
  },
  "scripts": {
        "auto-scripts": [],
    "post-install-cmd": [
        "@auto-scripts"
    ],
    "post-update-cmd": [
        "@auto-scripts"
    ]
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.1.*"
    }
  },
  "conflict": {
    "symfony/symfony": "5.1.*"
  }
}
